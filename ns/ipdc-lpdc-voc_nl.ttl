<http://fixme.com#address> a owl:ObjectProperty;
  rdfs:label "address"@nl;
  rdfs:comment "Adres dat men kan aanschrijven of bezoeken."@nl;
  rdfs:domain schemas:ContactPoint;
  rdfs:isDefinedBy <http://fixme.com>;
  rdfs:range <http://www.w3.org/ns/locn#Address> .

<http://fixme.com#competentAuthorityLevel> a owl:ObjectProperty;
